{
  "name": "serinc/utilities",
  "description": "Serinc Utilities For Alterative Method CURL With Guzzle HTTP & PSR And have Another Method For Encrypt & Decrypt Methods With Cipher & hexabin algorithm",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "http",
    "Request curl"
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*.*|7.*.*|8.*.*",
    "guzzlehttp/guzzle": "^6.0|^7.0"
  },
  "support": {
    "issues": "https://github.com/yahongie2014/SerincHttp/issues",
    "source": "https://github.com/yahongie2014/SerincHttp"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "serinc\\": "src/"
    }
  },
  "authors": [
    {
      "name": "coder79",
      "email": "a.saeed@null.net"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
