{
  "name": "serinc/utilities",
  "description": "Custom utilities",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "http",
    "Request curl"
  ],
  "require": {
    "php": ">=7.0.0",
    "ocramius/package-versions": "1.4.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*.*|7.*.*|8.*.*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "support": {
